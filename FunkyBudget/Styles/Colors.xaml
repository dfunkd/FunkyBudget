<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="LightestColor">#FF999999</Color>
    <Color x:Key="LighterColor">#FF595959</Color>
    <Color x:Key="DarkerColor">#FF323232</Color>
    <Color x:Key="DarkestColor">#FF0D0D0D</Color>
    <Color x:Key="RuanRedColor">#FFBD2036</Color>
    <Color x:Key="RuanGreenColor">#FF20BD36</Color>
    <Color x:Key="MissingColor">#FF614C4F</Color>
    <Color x:Key="MouseOverColor">#FFED929E</Color>
    <Color x:Key="TransparentColor">#00000000</Color>
    <Color x:Key="ComparisonForegroundColor">#FF999999</Color>
    <Color x:Key="PaidColor">#FF66FF66</Color>
    <Color x:Key="UnpaidColor">#FFFFFF66</Color>
    <Color x:Key="UnpaidDueColor">#FFFF6666</Color>

    <SolidColorBrush x:Key="Lightest" Color="{DynamicResource LightestColor}" />
    <SolidColorBrush x:Key="Lighter" Color="{DynamicResource LighterColor}" />
    <SolidColorBrush x:Key="Darker" Color="{DynamicResource DarkerColor}" />
    <SolidColorBrush x:Key="Darkest" Color="{DynamicResource DarkestColor}" />
    <SolidColorBrush x:Key="RuanRed" Color="{DynamicResource RuanRedColor}" />
    <SolidColorBrush x:Key="RuanGreen" Color="{DynamicResource RuanGreenColor}" />
    <SolidColorBrush x:Key="Missing" Color="{DynamicResource MissingColor}" />
    <SolidColorBrush x:Key="MouseOverBrush" Color="{DynamicResource MouseOverColor}" />
    <SolidColorBrush x:Key="TransparentBrush" Color="{DynamicResource TransparentColor}" />
    <SolidColorBrush x:Key="ComparisonForeground" Color="{DynamicResource ComparisonForegroundColor}" />
    <SolidColorBrush x:Key="Paid" Color="{DynamicResource PaidColor}" />
    <SolidColorBrush x:Key="Unpaid" Color="{DynamicResource UnpaidColor}" />
    <SolidColorBrush x:Key="UnpaidDue" Color="{DynamicResource UnpaidDueColor}" />

    <LinearGradientBrush x:Key="HeaderFade" StartPoint="0.5, 0" EndPoint="0.5, 1">
        <GradientStop Color="{DynamicResource LighterColor}" Offset="0" />
        <GradientStop Color="{DynamicResource DarkestColor}" Offset="1" />
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="ReverseHeaderFade" StartPoint="0.5, 0" EndPoint="0.5, 1">
        <GradientStop Color="{DynamicResource DarkestColor}" Offset="0" />
        <GradientStop Color="{DynamicResource LighterColor}" Offset="1" />
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="RuanGreenFade" StartPoint="0.5, 0" EndPoint="0.5, 1">
        <GradientStop Color="{DynamicResource RuanGreenColor}" Offset="0" />
        <GradientStop Color="#FF0D0D0D" Offset="1" />
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="RuanRedFade" StartPoint="0.5, 0" EndPoint="0.5, 1">
        <GradientStop Color="{DynamicResource RuanRedColor}" Offset="0" />
        <GradientStop Color="#FF0D0D0D" Offset="1" />
    </LinearGradientBrush>

    <!--Expander-->
    <SolidColorBrush x:Key="Expander.Static.Circle.Stroke" Color="{DynamicResource DarkestColor}" />
    <SolidColorBrush x:Key="Expander.Static.Circle.Fill" Color="{DynamicResource RuanRedColor}" />
    <SolidColorBrush x:Key="Expander.Static.Arrow.Stroke" Color="White" />
    <SolidColorBrush x:Key="Expander.MouseOver.Circle.Stroke" Color="{DynamicResource DarkestColor}" />
    <SolidColorBrush x:Key="Expander.MouseOver.Circle.Fill" Color="{DynamicResource RuanRedColor}" />
    <SolidColorBrush x:Key="Expander.MouseOver.Arrow.Stroke" Color="White" />
    <SolidColorBrush x:Key="Expander.Pressed.Circle.Stroke" Color="{DynamicResource DarkestColor}" />
    <SolidColorBrush x:Key="Expander.Pressed.Circle.Fill" Color="{DynamicResource DarkerColor}" />
    <SolidColorBrush x:Key="Expander.Pressed.Arrow.Stroke" Color="White" />
    <SolidColorBrush x:Key="Expander.Disabled.Circle.Stroke" Color="{DynamicResource DarkestColor}" />
    <SolidColorBrush x:Key="Expander.Disabled.Circle.Fill" Color="{DynamicResource LighterColor}" />
    <SolidColorBrush x:Key="Expander.Disabled.Arrow.Stroke" Color="{DynamicResource LightestColor}" />
</ResourceDictionary>